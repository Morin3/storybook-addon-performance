{
  "name": "storybook-addon-performance",
  "version": "0.16.1",
  "description": "A storybook addon to help better understand and debug performance for React components",
  "keywords": [
    "addon",
    "storybook",
    "performance",
    "storybook-addon",
    "test",
    "popular"
  ],
  "main": "dist/cjs/index.js",
  "types": "dist/esm/index.d.ts",
  "module": "dist/esm/index.js",
  "author": "Alex Reardon <areardon@atlassian.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/atlassian-labs/storybook-addon-performance.git"
  },
  "files": [
    "/dist",
    "/src",
    "register.js",
    "register.mjs"
  ],
  "dependencies": {
    "@storybook/addons": "^7.0.0-beta.2",
    "@storybook/api": "^7.0.0-beta.2",
    "@storybook/channels": "^7.0.0-beta.2",
    "@storybook/components": "^7.0.0-beta.2",
    "@storybook/core-events": "^7.0.0-beta.2",
    "@storybook/theming": "^7.0.0-beta.2",
    "@testing-library/dom": "^7.22.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@xstate/react": "^1.5.1",
    "gzip-js": "^0.3.2",
    "styled-components": "^5.1.1",
    "tiny-invariant": "^1.1.0",
    "xstate": "^4.22.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "devDependencies": {
    "@types/gzip-js": "^0.3.1",
    "@types/jest": "^26.0.9",
    "@types/react-select": "^3.0.16",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "cypress": "^4.12.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-select": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.4",
    "tslib": "^2.0.1",
    "typescript": "^3.9.7"
  },
  "config": {
    "prettier_target": "src/**/*.{ts,tsx,js,jsx,md,json} test/**/*.{ts,tsx,js,jsx,md,json} examples/**/*.{ts,tsx,js,jsx,md,json}"
  },
  "scripts": {
    "cypress:open": "yarn cypress open",
    "cypress:run": "yarn cypress run",
    "storybook:dev": "yarn storybook dev --port 9003",
    "storybook:build": "yarn build && yarn storybook build",
    "typescript:check": "yarn tsc --noEmit",
    "typescript:watch": "tsc --watch",
    "typescript:build": "tsc && tsc --build tsconfig.cjs.json",
    "prettier:check": "yarn prettier src --debug-check --config ../../.prettierrc",
    "prettier:write": "yarn prettier src --write --config ../../.prettierrc",
    "eslint:check": "eslint -c ../../.eslintrc.js --max-warnings=0",
    "validate": "yarn typescript:check && yarn prettier:check && yarn eslint:check",
    "dev": "yarn typescript:watch & yarn storybook:dev",
    "test": "yarn jest",
    "build:clean": "rimraf dist",
    "build": "yarn build:clean && yarn typescript:build",
    "preversion": "yarn test",
    "postversion": "git add -A && git commit -m \"chore: publishing new version\" && git push --tags",
    "prepublishOnly": "yarn build"
  },
  "storybook": {
    "icon": "https://pbs.twimg.com/profile_images/1118574145724399616/oIyuo8uz_400x400.png",
    "displayName": "Performance"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.d.ts",
      "types": "./dist/esm/index.d.ts"
    },
    "./register": {
      "require": "./register.js",
      "import": "./register.mjs",
      "types": "./dist/esm/index.d.ts"
    }
  }
}
