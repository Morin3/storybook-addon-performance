{
  "name": "storybook-addon-performance-cli",
  "version": "1.0.0",
  "contributors": [
    "Alex Hinds",
    "Safira Nugruho"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "storybook-addon-performance": "*"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "validate": "yarn prettier:check && yarn eslint:check",
    "typescript:check": "tsc --noEmit",
    "typescript:watch": "tsc --watch",
    "prettier:check": "yarn prettier src --debug-check --config ../../.prettierrc",
    "prettier:write": "yarn prettier src -write --config ../../.prettierrc",
    "eslint:check": "eslint -c ../../.eslintrc.js --max-warnings=0"
  },
  "bin": {
    "sb-perf": "dist/index.js"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "config": {
    "prettier_target": "src/**/*.{ts,tsx,js,jsx,md,json} test/**/*.{ts,tsx,js,jsx,md,json}"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^26.6.3",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
